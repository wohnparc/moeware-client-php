{
  "name": "wohnparc/moeware-client",
  "description": "The PHP client library for the Moeware GraphQL API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wohnparc Dev Team",
      "email": "dev@wohnparc.de"
    }
  ],
  "config": {
    "platform": {
      "php": "8.0.30"
    },
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "php": ">=8.0",
    "softonic/graphql-client": "^3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.20",
    "pestphp/pest": "^v4.0.0",
    "phpstan/phpstan": "^1.12.0",
    "friendsofphp/php-cs-fixer": "^3.64.0",
    "dominikb/composer-license-checker": "^2.6.0",
    "comcast/php-legal-licenses": "^1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Wohnparc\\Moeware\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
